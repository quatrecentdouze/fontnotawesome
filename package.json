{
  "name": "fontnotawesome",
  "version": "1.1.7",
  "description": "A lightweight React component library for fontnotawesome icons",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./icons": "./dist/icons.json",
    "./css/all.css": "./dist/all.css",
    "./css/solid.css": "./dist/solid.css",
    "./css/brands.css": "./dist/brands.css",
    "./css/light.css": "./dist/light.css",
    "./css/regular.css": "./dist/regular.css",
    "./css/thin.css": "./dist/thin.css",
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "img"
  ],
  "scripts": {
    "download-assets": "node scripts/download-assets.js",
    "build": "node scripts/download-assets.js && node scripts/generate-icons-list.js && tsc && npm run build:styles",
    "build:styles": "node scripts/copy-styles.js",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "icons",
    "fontnotawesome",
    "component"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quatrecentdouze/fontnotawesome.git"
  },
  "homepage": "https://fontnotawesome.tlmreact.dev",
  "bugs": {
    "url": "https://github.com/quatrecentdouze/fontnotawesome/issues"
  },
  "dependencies": {
    "unzipper": "^0.12.3"
  }
}
